<%= form_for(cat) do |f| %>
  <p>
    <%= label_tag(:name, "Name:") %>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= label_tag(:age, "Age:") %>
    <%= f.number_field :age %>
  </p>
  <p>
    <%= label_tag(:gender, "Gender:") %>
    <%= f.radio_button(:gender, "f") %>
    <%= label_tag(:gender, "Female") %>
    <%= f.radio_button(:gender, "m") %>
    <%= label_tag(:gender, "Male") %>
  </p>
  <p>
    <%= label_tag(:breed, "Breed:") %>
    <%= f.text_field :breed %>
  </p>
  <p>
    <%= label_tag(:color, "Color:") %>
    <%= f.text_field :color %>
 </p>
 <p>
    <%= label_tag(:personality, "Personality:") %>
    <%= f.text_field :personality %>
  </p>
  <p>
    <%= f.submit "Save" %>
  </p>
<% end %>
